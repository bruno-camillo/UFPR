program contrario;
var n,m: longint;

(* coloque aqui o codigo da sua funcao *)
function contrario(m, n : longint): boolean;
var inverso, cent, dez, uni, copia, cont, i: longint;

begin
    copia:= n;
    cont:= 0;
    while copia <> 0 do                 //conta o numero de digitos
        begin
            copia:= copia div 10;
            cont:= cont + 1;
        end;
        
    inverso := 0;
    i:= 1;
    while i <= cont do
        begin
            unidade := numero mod 10;
            resto := numero div 10;
            inverso := inversoâˆ—10 + unidade ;
            numero := resto ;
            i:= i + 1;
        end;

    if m = inverso then
        contrario := true
    else
        contrario := false;
end;

begin
    read (n,m);
    
    if contrario (n,m) then
        writeln (n,' eh o contrario de ',m)
    
    else
        writeln (n,' nao eh o contrario de ',m);
end.
